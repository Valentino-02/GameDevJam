[sub_resource type="Resource" id="Resource_tfj02"]
script = ExtResource("19_ba0so")
speaker = ExtResource("20_vconq")
dialogueLine = "Unfortunately, in our world, things aren't quite so easy. There's a lot of things that want to stop us."
metadata/_custom_type_script = "uid://wil8yv1k8und"

[sub_resource type="Resource" id="Resource_kk71q"]
script = ExtResource("19_ba0so")
speaker = ExtResource("21_3tgbd")
dialogueLine = "Mostly bad weather! Let me show you what to watch out for."
metadata/_custom_type_script = "uid://wil8yv1k8und"


[sub_resource type="Resource" id="Resource_gtxgq"]
script = ExtResource("19_ba0so")
speaker = ExtResource("21_3tgbd")
dialogueLine = "This is the Great Wind, it helps keep the islands afloat, but also makes our job harder."
metadata/_custom_type_script = "uid://wil8yv1k8und"

[sub_resource type="Resource" id="Resource_pki5s"]
script = ExtResource("19_ba0so")
speaker = ExtResource("20_vconq")
dialogueLine = "Be very careful if you want to go through it, prepare for a bumpy ride!"
metadata/_custom_type_script = "uid://wil8yv1k8und"

[sub_resource type="Resource" id="Resource_jttnv"]
script = ExtResource("18_lnrt8")

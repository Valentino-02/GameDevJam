metadata/_custom_type_script = "uid://dr4g3okg075lr"

[sub_resource type="Resource" id="Resource_3tvg4"]
script = ExtResource("19_ba0so")
speaker = ExtResource("21_3tgbd")
dialogueLine = "Oh look! Rain!"
metadata/_custom_type_script = "uid://wil8yv1k8und"

[sub_resource type="Resource" id="Resource_10i8x"]
script = ExtResource("19_ba0so")
speaker = ExtResource("20_vconq")
dialogueLine = "Hmm, it might seem nice to take a quick shower, but it makes our platform all slippery. We always drop things in the rainstorms."
metadata/_custom_type_script = "uid://wil8yv1k8und"

[sub_resource type="Resource" id="Resource_ls6mm"]
script = ExtResource("18_lnrt8")
cameraToUse = &"Rain"
dialogues = Array[ExtResource("19_ba0so")]([SubResource("Resource_3tvg4"), SubResource("Resource_10i8x")])
childToTrigger = "DemonstrateRain"
metadata/_custom_type_script = "uid://dr4g3okg075lr"

[sub_resource type="Resource" id="Resource_1cus8"]
script = ExtResource("19_ba0so")
speaker = ExtResource("21_3tgbd")
dialogueLine = "Watch out for this land. As the cores get worse, the land starts to burn or freeze! You need to put this land out with some [color=blue]Water Cargo[/color]"
metadata/_custom_type_script = "uid://wil8yv1k8und"

[sub_resource type="Resource" id="Resource_u4b4b"]
script = ExtResource("18_lnrt8")
cameraToUse = &"Hazard1"
dialogues = Array[ExtResource("19_ba0so")]([SubResource("Resource_1cus8")])
childToTrigger = "DemonstrateDangerLand"
metadata/_custom_type_script = "uid://dr4g3okg075lr"

[sub_resource type="Resource" id="Resource_5frwg"]
script = ExtResource("19_ba0so")
speaker = ExtResource("20_vconq")
dialogueLine = "Just like this here, it's getting really cold, you need to thaw it with some [color=red]Fire cargo[/color]"
metadata/_custom_type_script = "uid://wil8yv1k8und"

[sub_resource type="Resource" id="Resource_imm8w"]
script = ExtResource("19_ba0so")
speaker = ExtResource("20_vconq")
dialogueLine = "Fixing the land like this will buy you more time by keeping the world balanced and safe"
metadata/_custom_type_script = "uid://wil8yv1k8und"

[sub_resource type="Resource" id="Resource_cqj2p"]
script = ExtResource("18_lnrt8")
cameraToUse = &"Hazard2"
dialogues = Array[ExtResource("19_ba0so")]([SubResource("Resource_5frwg"), SubResource("Resource_imm8w")])

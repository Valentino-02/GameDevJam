[sub_resource type="Resource" id="Resource_scl4b"]
script = ExtResource("19_ba0so")
speaker = ExtResource("21_3tgbd")
dialogueLine = "Which reminds me. If you focus too much on one core, or can't deliver packages fast enough, the world will start to fall apart."
metadata/_custom_type_script = "uid://wil8yv1k8und"

[sub_resource type="Resource" id="Resource_0l6fk"]
script = ExtResource("19_ba0so")
speaker = ExtResource("20_vconq")
dialogueLine = "Yeah, and as the world becomes more volatile, it'll throw things at you to make your life a lot spicier."
metadata/_custom_type_script = "uid://wil8yv1k8und"

[sub_resource type="Resource" id="Resource_gtqnh"]
script = ExtResource("19_ba0so")
speaker = ExtResource("20_vconq")
dialogueLine = "See if you can keep these cores cool and reach the goal without losing to much."
